   "source": [
    "import pandas as pd\n",
    "maxi = 6\n",
    "\n",
    "def f(x):\n",
    "    return x*2**x\n",
    "\n",
    "summanden = [[f(n), f(n)] for n in range(maxi)]\n",
    "\n",
    "df = pd.DataFrame(\n",
    "        summanden,\n",
    "        index = [n for n in range(maxi)], \n",
    "        columns = ['binary', 'decimal'])\n",
    "\n",
    "df['binary']=df['binary'].map('{:b}'.format)\n",
    "df"
   ]
