   ],
   "source": [
    "from itertools import takewhile\n",
    "\n",
    "list = [3,4,5,6,7,8,9]\n",
    "for i, item in takewhile(lambda x: x[0]<5, enumerate(list)):\n",
    "    print(i, item)"
   ]
  },
  {
   "cell_type": "code",
